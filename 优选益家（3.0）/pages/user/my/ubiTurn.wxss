.fixedBox {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  top: 0;
  left: 0;
  z-index: 2;
}

.fixedBox .guige {
  position: absolute;
  width: 100%;
  background-color: #fff;
  bottom: 0rpx;
  /* padding: 20px 0; */
  left: 0;
  /* padding-bottom: 45px; */
}

.fixedBox .ftitle {
  margin: 18rpx 14rpx 0rpx 48rpx;
  height: 107rpx;
}

.fixedBox .ftitle .guanbi .iconfont {
  font-size: 64rpx;
}

.fixedBox .box {
  padding: 20rpx 24rpx 20rpx 48rpx;
}

.fixedBox .list {
  height: 124rpx;
}

.fixedBox .list .img image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 24rpx;
}

.fixedBox .list .name {
  color: #333;
}

.fixedBox .list .vip image {
  width: 112rpx;
  height: 40rpx;
}

.fixedBox .list .select {
  width: 116rpx;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: rgba(50, 60, 88, 1);
  border-radius: 4rpx;
}
