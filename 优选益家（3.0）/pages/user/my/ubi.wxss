/* pages/user/my/ubi.wxss */
.head{
  height: 200rpx;
  background: #f5a623;
  padding: 0 40rpx 0 68rpx;
  color: #fff;
}

.head .num{
  font-size: 64rpx;
  padding:0 0 10rpx 10rpx;

}
.head .dq{
  font-size: 24rpx;
}

.head .zyBox{
  padding-top: 24rpx;
}
.head .zyBox .wenhao{
  width: 34rpx;
  height: 34rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 34rpx;
  border: 2rpx solid #fff;
  font-size: 24rpx;
  margin-right: 14rpx;

}
.head .zyBox .sm{
  font-size: 24rpx;
}
.head .zyBox .zyhy{
  margin: 24rpx 0 0 0;
  width:202rpx;
height:80rpx;
background:rgba(255,255,255,1);
border-radius:40rpx;
color: #F5A623;
