/* pages/user/sharePage.wxss */

.fixedBox {
  position: fixed;
  width: 100%;
  height: 100%;
  background: white;
  top: 0;
  left: 0;
  z-index: 2;
}
.sharBox {
  position: absolute;
  width: 100%;
  left: 0;
  bottom:0;
  padding:0rpx 0 200rpx 0;
}

.fxActionBox {
  text-align: center;
  background: #fff;
  position: fixed;
  bottom: 0;
  width: 100%;
  padding: 30rpx 0;
}

.fxActionBox image {
  width: 96rpx;
  height: 96rpx;
}

.canvas{
  width: 750rpx;
  height: 950rpx;
  
  /* border: solid 1px red; */
  /* margin:0 0 60rpx 0; */
}
.shareImg{
  width: 654rpx;
  height: 880rpx;
  z-index:9999;
  margin:0 0 60rpx 0;
}

.fxActionBox button{
  padding: 0;
  line-height:1;
  background: #fff;
  font-size: 26rpx;
}

button::after{
  border:0;
}